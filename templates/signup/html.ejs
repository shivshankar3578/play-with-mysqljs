                            <table width="600" align="center" cellpadding="0" cellspacing="0" border="0">
                            <![endif]-->
                            <tbody>
                                <tr>
                                    <td style="padding: 5px; margin: 0; font-family: Arial, Helvetica, sans-serif; font-size: 14px;" align="left">Name</td>
                                    <td style="padding: 5px; margin: 0; font-family: Arial, Helvetica, sans-serif; font-size: 14px; color: #1bb5e9;" align="left">
                                        <%= custom.name %>
                                    </td>
                                </tr>
                              <!--  <tr>
                                    <td style="padding: 5px; margin: 0; font-family: Arial, Helvetica, sans-serif; font-size: 14px;" align="left">Mobile Number</td>
                                    <td style="padding: 5px; margin: 0; font-family: Arial, Helvetica, sans-serif; font-size: 14px; color: #1bb5e9;" align="left">
                                        <%= custom.countryCode %> <%= custom.mobile %>
                                    </td>
                                </tr> -->
                                <tr>
                                    <td style="padding: 5px; margin: 0; font-family: Arial, Helvetica, sans-serif; font-size: 14px;" align="left">Email Address</td>
                                    <td style="padding: 5px; margin: 0; font-family: Arial, Helvetica, sans-serif; font-size: 14px; color: #1bb5e9;" align="left">
                                        <%= custom.email %>
                                    </td>
                                </tr>

                                <tr>
                                    <td style="padding: 5px; margin: 0; font-family: Arial, Helvetica, sans-serif; font-size: 14px;" align="left">Password</td>
                                    <td style="padding: 5px; margin: 0; font-family: Arial, Helvetica, sans-serif; font-size: 14px; color: #1bb5e9;" align="left">We strongly recommend that you do not disclose your password to anyone.</td>
                                </tr>
                                <!-- [if(gte mso 9)|(IE)]>
                                </table>
                                <![endif]-->
                            </tbody>
                        </table>
                    </div>
                </td>
            </tr>
            <tr style="background-color: #fff;">
                <td>&nbsp;</td>
            </tr>
            <tr style="background-color: #fff;">
                <td>
                    <div class="editabletemplatebox" style="font-family: Arial, Helvetica, sans-serif; font-size: 14px; color: #000000; line-height: 17px; padding: 0 30px;">
                        <table style="border: 0px solid #ddd; border-collapse: collapse;" border="0" width="100%" cellspacing="0" cellpadding="0" align="center">
